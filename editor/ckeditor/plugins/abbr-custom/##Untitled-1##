		console.log('enter finalize');
		$.transform({
				async: false,
				el:"#hidden",
				xmlobj:xml, 
				xsl: pathToXSL +  "group.xsl",
				error: function(html,xsl,xml,object,e) {
								console.log('fi transformation failed, ' + e);
								console.log("***" + html );
				},
				success: function(html,xsl,xml,object,e) {
								var asString = (new XMLSerializer()).serializeToString(xml);
								console.log("**" + html + xml);
								console.log("**" + asString);
				}
		});
